      #{deviceNo,jdbcType=VARCHAR}, #{themeNo,jdbcType=VARCHAR}, #{gatewayNo,jdbcType=VARCHAR},#{deviceState,jdbcType=VARCHAR},#{scheduleName,jdbcType=VARCHAR}, 
